import java.util.*;
import java.time.*;

public class Header {

    private Month month;

    public Header (Month month) {
        setMonth(month);
    }

// ----------------------------------------------------------------------------------------------------------------
//  ****************** Setters ******************
// ----------------------------------------------------------------------------------------------------------------

    private void setMonth(Month month) {
        this.month = month;
    }

// ----------------------------------------------------------------------------------------------------------------
//  ****************** Getters ******************
// ----------------------------------------------------------------------------------------------------------------

    public Month getMonth() {
        return month;
    }

}// Header
